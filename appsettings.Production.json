{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Error"
    }
  },
  "ConnectionStrings": {
    "Default": "Server=PRODUCTION_SERVER;Database=GrcDb;Trusted_Connection=True;TrustServerCertificate=True;MultipleActiveResultSets=true"
  },
  "Environment": {
    "Name": "Production"
  },
  "Cors": {
    "AllowedOrigins": [
      "https://grc.yourdomain.com"
    ],
    "AllowCredentials": true
  },
  "Authentication": {
    "Jwt": {
      "SecretKey": "CHANGE_THIS_IN_PRODUCTION_ENVIRONMENT_VARIABLE",
      "Expiration": 3600
    }
  }
}
